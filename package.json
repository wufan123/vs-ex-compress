{
  "name": "vs-ex-compress",
  "displayName": "vs-ex-compress",
  "version": "0.0.2",
  "publisher": "waslong",
  "license": "MIT",
  "repository": "https://github.com/wufan123/vs-ex-compress",
  "icon": "media/logo.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "recentFiles",
          "name": "%recentFiles%",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "%compressSettings%",
      "properties": {
        "compress.m01.ignore": {
          "type": "string",
          "default": ".*\\.(zip|rar|scss|less|map)$|\\.vscode$|node_modules",
          "description": "%compress.ignore%"
        }
      }
    },
    "commands": [
      {
        "command": "vs-ex-compress.refreshRecentFiles",
        "title": "%compress.refreshRecentFiles%"
      },
      {
        "command": "vs-ex-compress.compressDirectory",
        "title": "%compress.compressDirectory%",
        "icon": "media/zip1.svg"
      },
      {
        "command": "vs-ex-compress.compressSelectedItems",
        "title": "%compress.compressSelectedItems%",
        "icon": "media/zip2.svg"
      },
      {
        "command": "vs-ex-compress.openSettings",
        "title": "%compress.openSettings%"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "vs-ex-compress.compressSelectedItems",
          "group": "navigation",
          "when": "view == 'recentFiles'"
        }
      ],
      "view/title": [
        {
          "command": "vs-ex-compress.compressDirectory",
          "group": "navigation",
          "when": "view == 'recentFiles'"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "recentFiles",
        "contents": "%viewsWelcome.noRecentFiles%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "push": "git push origin && git push github",
    "vb": "vsce package",
    "vp": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "vscode-nls-i18n": "^0.2.4"
  },
  "vscode-nls-i18n": {
    "defaultLanguage": "zh-cn"
  }
}
